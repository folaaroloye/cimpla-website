    "postcss": "^8",
    "husky": "^8.0.3",
    "tailwindcss": "^3.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
